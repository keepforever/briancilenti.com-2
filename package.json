    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gsap": "^3.12.5",
    "isbot": "^5.1.31",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.2",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2"
