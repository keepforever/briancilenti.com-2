    "clsx": "^2.1.1",
    "isbot": "^5.1.32",
    "lucide-react": "^0.556.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router": "^7.10.1",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2"
